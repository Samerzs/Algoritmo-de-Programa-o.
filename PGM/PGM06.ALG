Algoritmo "PGM6"

Var
      media,km,consumo : real

Inicio

      escreva("Digite aqui a distância percorrida(em km) pelo automóvel: ")
      leia(km)
      escreva("Digite o total de combustivel gasto:  ")
      leia(consumo)
      
      media <-  (km/consumo)
      
      escreva("O Consumo total gasto foi de: ",media)





Fimalgoritmo